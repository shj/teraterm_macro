;
; すべてのマクロで共有する設定
; ここでの設定はdefault.ttlの設定内容を上書きします
;

; COMポート番号
comport = 3

; エラー発生時、エラーメッセージを表示しなくない場合は0以外に設定する
nomessage_flag = 0

; ログファイル名の拡張子
logsuffix = '.log'

; マクロ終了時にログを閉じる（自動的に閉じる場合は 1、閉じない場合は 0）
logautoclosemode_flag = 1

; ログファイル名に日付を付加する（日付を付加する場合は 1、しない場合は 0 ）
logname_with_datetime = 1
